# Ejemplo de archivo .env para Kaliversus API

# Puerto de la API
PORT=3000

# Entorno
NODE_ENV=development

# Base de datos PostgreSQL
DATABASE_URL=postgres://usuario:password@localhost:5432/kaliversus

# JWT
JWT_SECRET=supersecreto
JWT_EXPIRES_IN=1d

# RabbitMQ
RABBITMQ_URL=amqp://localhost

# Resend (email)
RESEND_API_KEY=tu_api_key_de_resend

# Otros
# ...agrega aqu√≠ cualquier variable adicional necesaria
